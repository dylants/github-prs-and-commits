{
  "name": "github-prs-and-commits",
  "version": "0.0.0",
  "description": "Returns info for the open PRs in a GitHub repo",
  "engines": {
    "node": ">=18",
    "yarn": "~1"
  },
  "private": true,
  "scripts": {
    "build:ts": "tsc",
    "build": "yarn clean:build && yarn build:ts",
    "clean:build": "rimraf build",
    "clean:dependencies": "rimraf node_modules",
    "clean": "yarn clean:build && yarn clean:dependencies",
    "lint": "eslint .",
    "prod": "node build",
    "start": "nodemon src"
  },
  "dependencies": {},
  "devDependencies": {
    "@tsconfig/node18": "1.0.1",
    "@types/node": "18.11.7",
    "@typescript-eslint/eslint-plugin": "5.41.0",
    "@typescript-eslint/parser": "5.41.0",
    "eslint": "8.26.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-prettier": "4.2.1",
    "nodemon": "2.0.20",
    "prettier": "2.7.1",
    "ts-node": "10.9.1",
    "typescript": "4.8.4"
  }
}
